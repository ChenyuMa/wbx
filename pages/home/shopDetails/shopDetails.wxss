.click-shop {
  width: 100%;
  height: 8%;
  background: gainsboro;
  font-size: 30rpx;
  text-align: center;
  justify-content: center;
  position: fixed;
  bottom: 0;
}

/********************************商品多规格  */

.goods-attr {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.3);
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
}

.attr {
  width: 80%;
  height: 50%;
  background: whitesmoke;
  /* margin-left: 7.5%; */
  /* margin-top: 35%; */
  border-radius: 10rpx;
}

/*头部  */

.attr-top {
  width: 100%;
  height: 15%;
  justify-content: center;
  align-items: center;
  position: relative;
}

.attr-top text:first-child {
  font-size: 30rpx;
}

.attr-top text:last-child {
  position: absolute;
  right: 20rpx;
  top: 10rpx;
  font-size: 40rpx;
  color: #10bfaf;
}

/*规格  */

.attr-select {
  width: 100%;
  height: 70%;
}

.attr-select text {
  font-size: 30rpx;
  margin-left: 20rpx;
}

.attr-select-details {
  /* display: flex;
  width: 100%;
  flex-direction: column;
  background: rebeccapurple; */
  margin-top: 10rpx;
}

.select_goods_attrs {
  border: gray solid 1rpx;
  padding: 3rpx 10rpx;
  border-radius: 30rpx;
  /* margin-top: 20rpx; */
  /* width: 50%; */
  text-align: center;
}

.select_goods_attr {
  /* margin-top: 20rpx; */
  background: #10bfaf;
  border-radius: 20rpx;
  padding: 3rpx 10rpx;
  color: white;
  border: solid #10bfaf 1rpx;
  text-align: center;
}

.select_attr_seckill {
  color: red;
}
/*多规格价格  */

.attr-price {
  width: 100%;
  height: 15%;
  background: whitesmoke;
  align-items: center;
  justify-content: space-between;
}

.attr-price-select {
  color: gray;
  margin-left: 20rpx;
}
.attr-price-member{
  color: red;
}

/*商品添加  */

.goods-attr-add {
  width: 30%;
  height: 100%;
  align-items: center;
  margin-right: 20rpx;
  justify-content: space-between;
}

.goods-attr-add image {
  width: 50rpx;
  height: 50rpx;
}

/******************************订桌  */

.setTable {
  position: absolute;
  width: 80rpx;
  height: 80rpx;
  right: 0;
  top: 25%;
}

.setTable image {
  width: 100%;
  height: 100%;
}

/*智能付款  */

.smartPay {
  width: 70rpx;
  height: 70rpx;
  margin-left: -100rpx;   
}

.smartPay image {
  width: 100%;
  height: 100%;
}

/*扫码点餐右侧  */

.goodsStyles {
  width: 94%;
  height: 400rpx;
  margin-left: 3%;
  background-image: url('http://www.wbx365.com/static/default/wap/image/xiaochengxu/frame@2x.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}

/*右侧商品标题  */

.goodsTitle {
  width: 80%;
  margin: 3% 0 0 10%;
}

.goodsTitle text {
}

/*商品图片  */

.goodsPhoto {
  width: 80%;
  height: 60%;
  margin: 2% 0 2% 10%;
  position: relative;
}

/*价格数量  */

.goodsPriceNum {
  width: 80%;
  margin-left: 10%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.goodsPrice {
  width: 50%;
  color: gray;
}

.goodsNums {
  width: 50%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.goodsNums text:nth-child(1) {
  color: #06c1ae;
  font-size: 50rpx;
}

.goodsNums text:nth-child(2) {
  color: gray;
}

.goodsNums text:nth-child(3) {
  color: #06c1ae;
  font-size: 50rpx;
}

/* 店铺评论 */
.shop-discuss{
  width: 100%;
  height: 75%;
  background: white;
  position: absolute;
  bottom: 0;
}
.discuss{
  width: 100%;
  display: flex;
  flex-direction: row;
  border: 1rpx solid whitesmoke;
}
.discuss-img{
  width: 20%;
  /* height: 100%; */
  display: flex;
  justify-content: center;
  align-items: center;
}
.discuss-img image{
  width: 100rpx;
  height: 100rpx;
  background: gray;
  border-radius: 50rpx;
}
.discuss-content{
  width: 80%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 30rpx;
}

/* 消息 */
.messaes{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 30rpx;
  top: -40rpx;
  z-index: 30;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 60rpx;
  background: white;
  border: 1rpx solid whitesmoke;
}
.messaes image{
  width: 40rpx;
  height: 40rpx;
  border-radius: 50rpx;
}

/* VIP规格 */
.vip-rules{
  font-size: 25rpx;
  color: white;
}

/* 优惠活动 */
.favourable{
  width: 100%;
  height: 100%;
  z-index: 30;
  background-color: rgba(0, 0, 0, 0.4);
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
}
.favourable-content{
  width: 100%;
  height: 50%;
  background: white;
  align-self: flex-end;
}
.content-titel{
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.content-titel text:first-child{
  margin:0 auto;
}
.content-titel image{
  width: 40rpx;
  height: 40rpx;
  margin-right: 40rpx;
}
.content-info{
  font-size: 30rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
}
.info-left{
  background: red;
  padding: 5rpx 10rpx;
  border-radius: 5rpx;
  margin-left: 20rpx;
  color: white;
  
}

/* 优惠券新样式 */
.downCoupons {
  width: 100%;
  height: 50%;
  background: #f5f5f5;
  align-self: flex-end;
}
.downCoupons-titel {
  width: 100%;
  height: 20%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.couponsClose {
  color: gray;
  position: absolute;
  right: 10px;
}
.downCoupons-titels {
  height: 10%;
  display: flex;
  align-items: center;
}
.coupons-img {
  margin: 0 auto;
  margin-bottom: 50rpx;
  width: 600rpx;
  height: 200rpx;
  background-image: url('http://www.wbx365.com/static/default/wap/image/xiaochengxu/cover@2x.png');
  background-size: 100% 100%;
  display: flex;
  flex-direction: row;
  position: relative;
}

.img-price {
  width: 22%;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.img-price text:nth-child(1) {
  color: white;
}

.img-price text:nth-child(2) {
  color: white;
  font-size: 60rpx;
}
.img-fullCoupons {
  width: 45%;
  height: 100%;
  display: flex;
  align-items: center;
}

.img-fullCoupons text:nth-child(1) {
  font-size: 35rpx;
  margin-left: 3%;
  position: absolute;
  top: 35%;
  color: #1fbdb1;
}

.img-fullCoupons text:nth-child(2) {
  font-size: 20rpx;
  position: absolute;
  top: 65%;
  color: white;
}
.img-to {
  width: 33%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}

.to {
  width: 110rpx;
  height: 50rpx;
  background: #1fbdb1;
  text-align: center;
  color: white;
  font-size: 35rpx;
  line-height: 50rpx;
  border-radius: 25rpx;
  position: absolute;
  padding: 0 10rpx;
  left: 12%;
}

.tos {
  background: gray;
}
/*************微信登录  */

.wx-longin {
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 100;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.7);
  display: flex;
  justify-content: center;
  align-items: center;
}

.login-content {
  width: 600rpx;
  height: 500rpx;
  background: #fff;
  border-radius: 10rpx;
}

.longin-title {
  width: 100%;
  height: 18%;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom: 4rpx solid #f4f4f5;
  color: #464646;
}

.longin-icon {
  width: 100%;
  height: 59.5%;
  border-bottom: 4rpx solid #f4f4f5;
  display: flex;
  /* justify-content: center; */
  flex-direction: column;
  align-items: center;
}

.longin-icon image {
  width: 80rpx;
  height: 80rpx;
  margin-top: 30rpx;
}

.title1 {
  margin-top: 20rpx;
  font-size: 35rpx;
}

.line {
  width: 80%;
  margin-top: 20rpx;
  border-bottom: 6rpx solid #f4f4f5;
}

.titel2 {
  margin-top: 20rpx;
  font-size: 25rpx;
  color: gray;
}

.btn {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 18%;
}

.btn1 {
  width: 50%;
  border-right: 1rpx solid #f4f4f5;
}

.btn2 {
  width: 50%;
  border-left: 1rpx solid #f4f4f5;
  color: #0bb20c;
}

.btn button {
  background: white;
}

button::after {
  border: none;
}
